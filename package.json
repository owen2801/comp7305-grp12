{
  "name": "comp7305-grp12",
  "version": "1.0.0",
  "description": "COMP7305 Group12 Stack Overflow Data Analysis",
  "author": "comp7305-grp12",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "org.hku.comp7305.grp12",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS"
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-builder": "^20.10.0"
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "bootstrap": "^4.1.0",
    "echarts": "^4.0.4",
    "froala-editor": "^2.8.0",
    "install": "^0.11.0",
    "jquery": "^3.3.1",
    "mustache": "^2.3.0",
    "mysql": "^2.15.0",
    "npm": "^5.8.0",
    "popper.js": "^1.14.3",
    "underscore": "^1.9.0"
  }
}